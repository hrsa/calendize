{
    "name": "hrsa/calendize",
    "type": "project",
    "description": "The easiest way to turn e-mails into ICS files.",
    "keywords": ["calendize", "ics", "calendar", "email"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-pdo": "*",
        "ext-zip": "*",
        "beyondcode/laravel-mailbox": "^4.0",
        "inertiajs/inertia-laravel": "^1.1",
        "laravel-notification-channels/telegram": "^5.0",
        "laravel/framework": "^11.7",
        "laravel/horizon": "^5.24.4",
        "laravel/pulse": "^1.1",
        "laravel/reverb": "@beta",
        "laravel/sanctum": "^4.0.2",
        "laravel/socialite": "^5.14",
        "laravel/tinker": "^2.9",
        "lemonsqueezy/laravel": "^1.5.3",
        "masbug/flysystem-google-drive-ext": "^2.3",
        "openai-php/laravel": "^0.8.1",
        "socialiteproviders/zoho": "^4.1",
        "spatie/laravel-backup": "^8.8",
        "spatie/laravel-ray": "^1.36.2",
        "symfony/http-client": "^7.0.7",
        "tightenco/ziggy": "^2.2.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23.1",
        "larastan/larastan": "^2.9.6",
        "laravel/breeze": "^2.0.3",
        "laravel/pint": "^1.15.3",
        "laravel/sail": "^1.29.1",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.1.1",
        "nunomaduro/phpinsights": "^2.11",
        "pestphp/pest": "^2.34.7",
        "pestphp/pest-plugin-laravel": "^2.4",
        "spatie/laravel-ignition": "^2.7.0",
        "spatie/laravel-sitemap": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "pint": [
            "./vendor/bin/pint"
        ],
        "coverage": [
            "./vendor/bin/pest --coverage"
        ],
        "test": [
            "./vendor/bin/pest"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
